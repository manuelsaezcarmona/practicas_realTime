# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
# Set default charset
charset = utf-8
# 4 space indentation
indent_size = 2
# Tab indentation (no size specified)
indent_style = space
# Matches the exact files either package.json or .travis.yml
trim_trailing_whitespace = true
